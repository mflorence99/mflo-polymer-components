{
  "globals": {
    "_": true,
    "console": true,
    "escape": true,
    "unescape": true
  },
  "rules": {
    "camelcase": [0],
    "curly": [0],
    "dot-notation": [0],
    "eqeqeq": [0],
    "key-spacing": [0],
    "new-cap": [0],
    "no-console": [0],
    "no-constant-condition": [0],
    "no-empty": [0],
    "no-shadow": [0],
    "no-trailing-spaces": [0],
    "no-underscore-dangle": [0],
    "no-use-before-define": [2, "nofunc"],
    "space-infix-ops": [0],
    "strict": [0]
  }
}
